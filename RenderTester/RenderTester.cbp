<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="RenderTester" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Release">
				<Option output="..\bin\RenderTester" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\bin\" />
				<Option object_output=".obj\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="irrRenderer" />
				</Linker>
			</Target>
			<Target title="Debug">
				<Option output="..\bin\RenderTester" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\bin\" />
				<Option object_output=".obj\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library="..\lib\debug\irrRenderer.a" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-fexceptions" />
			<Add directory="$(#irrlicht.include)" />
			<Add directory="$(#irrrenderer.include)" />
		</Compiler>
		<Linker>
			<Add library="Irrlicht" />
			<Add directory="$(#irrlicht.include)" />
			<Add directory="..\bin" />
		</Linker>
		<Unit filename="CTestFramework.cpp" />
		<Unit filename="CTestFramework.h" />
		<Unit filename="main.cpp" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
