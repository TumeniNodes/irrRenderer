<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="RenderTest" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Release">
				<Option output="..\bin\RenderTest" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\bin\" />
				<Option object_output=".obj\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-fexpensive-optimizations" />
					<Add option="-O3" />
					<Add directory="$(#irrlicht.include)" />
					<Add directory="$(#irrrenderer.include)" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="Irrlicht" />
					<Add library="$(#irrrenderer.lib)\libirrRenderer.a" />
				</Linker>
			</Target>
			<Target title="Debug">
				<Option output="..\bin\RenderTest" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\bin\" />
				<Option object_output=".obj\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O" />
					<Add option="-pg" />
					<Add option="-g" />
					<Add directory="$(#irrlicht.include)" />
					<Add directory="$(#irrrenderer.include)" />
					<Add directory="..\..\deferred\include" />
				</Compiler>
				<Linker>
					<Add option="-pg -lgmon" />
					<Add library="Irrlicht" />
					<Add library="$(#irrrendererdebug.lib)\libirrRenderer.a" />
					<Add library="..\..\deferred\lib\win32-gcc\libIrrlichtDeferredRendering.dll.a" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-fexceptions" />
		</Compiler>
		<Linker>
			<Add directory="$(#irrlicht.include)" />
			<Add directory="..\bin" />
		</Linker>
		<Unit filename="CTestFramework.cpp" />
		<Unit filename="CTestFramework.h" />
		<Unit filename="main.cpp" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
